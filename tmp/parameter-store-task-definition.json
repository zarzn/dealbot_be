{
    "family": "agentic-deals-tasks",
    "containerDefinitions": [
        {
            "name": "agentic-deals-app",
            "image": "586794462529.dkr.ecr.us-east-1.amazonaws.com/agentic-deals-backend:latest",
            "cpu": 1024,
            "memory": 2048,
            "memoryReservation": 1024,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "name": "agentic-deals-app-8000-tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "ALLOWED_ORIGINS",
                    "value": "*"
                },
                {
                    "name": "APP_ENVIRONMENT",
                    "value": "production"
                },
                {
                    "name": "LOG_LEVEL",
                    "value": "INFO"
                },
                {
                    "name": "USE_PARAMETER_STORE",
                    "value": "true"
                },
                {
                    "name": "USE_SECRETS_MANAGER",
                    "value": "false"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "POSTGRES_DB",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/POSTGRES_DB"
                },
                {
                    "name": "POSTGRES_HOST",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/POSTGRES_HOST"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/POSTGRES_PASSWORD"
                },
                {
                    "name": "POSTGRES_PORT",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/POSTGRES_PORT"
                },
                {
                    "name": "POSTGRES_USER",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/POSTGRES_USER"
                },
                {
                    "name": "REDIS_HOST",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/REDIS_HOST"
                },
                {
                    "name": "REDIS_PASSWORD",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/REDIS_PASSWORD"
                },
                {
                    "name": "REDIS_PORT",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/REDIS_PORT"
                },
                {
                    "name": "SECRET_KEY",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/SECRET_KEY"
                },
                {
                    "name": "SCRAPER_API_KEY",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/SCRAPER_API_KEY"
                },
                {
                    "name": "DEEPSEEK_API_KEY",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/DEEPSEEK_API_KEY"
                },
                {
                    "name": "OPENAI_API_KEY",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/OPENAI_API_KEY"
                },
                {
                    "name": "RESET_DB",
                    "valueFrom": "arn:aws:ssm:us-east-1:586794462529:parameter/agentic-deals/RESET_DB"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/agentic-deals-container",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -s http://localhost:8000/health"
                ],
                "interval": 15,
                "timeout": 10,
                "retries": 3,
                "startPeriod": 150
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::586794462529:role/agentic-deals-task-role",
    "executionRoleArn": "arn:aws:iam::586794462529:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048"
} 